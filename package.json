    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "browserify-fs": "^1.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "dotenv": "^6.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "expect": "^23.6.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs": "0.0.1-security",
    "fs-extra": "^6.0.1",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.11.0",
    "nyc": "^13.1.0",
    "path": "^0.12.7",
